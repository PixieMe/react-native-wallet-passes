    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "endOfLine": "lf"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
