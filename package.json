    "url": "git+https://github.com/TheWirv/react-native-wallet-passes.git"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "PassKit",
    "Wallet"
  ],
  "author": {
    "email": "miyabi@mybdesign.com",
    "name": "Masayuki Iwai",
    "url": "http://mybdesign.com"
  },
  "contributors": [
    {
      "email": "xxx.mazzel.xxx+github@gmail.com",
      "name": "Marcel Lasaj"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/TheWirv/react-native-wallet-passes#readme",
  "dependencies": {
    "react-native": "^0.64.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@types/react": "^17.0.11",
    "@types/react-native": "^0.64.10",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "react": "^17.0.2",
    "react-native-builder-bob": "^0.18.1",
    "typescript": "^4.3.5"
